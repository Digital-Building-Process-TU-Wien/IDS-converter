                    <propertySet>
                        <simpleValue>Pset_CoveringCommon</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>Status</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
