                    </propertySet>
                    <baseName>
                        <simpleValue>NumberOfRiser</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCCOUNTMEASURE" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/NumberOfTreads" cardinality="required" instructions="Gesamtzahl der Auftritte, die in der Treppe enthalten sind.">
